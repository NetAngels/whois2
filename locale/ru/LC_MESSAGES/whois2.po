#: whois2/__init__.py:35
msgid "invalid domain name"
msgstr "неверное доменное имя"

#: whois2/__init__.py:39
msgid "there are no rules to handle .{} domains"
msgstr "нет правил для обработки доменов .{}"

#: whois2/validators.py:16
msgid "pick 2nd level domain. domain.tld, not www.domain.tld"
msgstr "Укажите домен второго уровня. domain.ru, а не www.domain.ru"

#: whois2/validators.py:19
msgid "domains must contain letters, numbers and a \"-\" sign only"
msgstr "домены могут содержать только русские буквы, цифры и символ  \"-\""

#: whois2/validators.py:21 whois2/validators.py:64
msgid "domain cannot starts or ends with \"-\" sign"
msgstr "домены не могут начинаться или заканчиваться символом  \"-\""

#: whois2/validators.py:29
msgid "IDNA domains are forbidden in .{} domain"
msgstr "В зоне .{} запрещены домены IDNA"

#: whois2/validators.py:41
msgid "domain names shorter than 3 letter are forbidden in .{}"
msgstr "доменные имена короче 3 символов запрещены в .{}"

#: whois2/validators.py:48
msgid "domain names shorter than 2 letter are forbidden in .{}"
msgstr "доменные имена короче 2 символов запрещены в .{}"

#: whois2/validators.py:57
msgid "RF domain must contain at least 2 symbols"
msgstr "домены РФ должны иметь как минимум два символа"

#: whois2/validators.py:59
msgid "IDNA representation of the domain contains more than 63 symbols"
msgstr "IDNA-представление домена содержит более 63 символов"

#: whois2/validators.py:62
msgid "RF domain must contain only russian letters, numbers and a \"-\" sign"
msgstr "домены РФ должны содержать только русские буквы, цифры и символ  \"-\""

#: scripts/whois2:12
msgid "don't show the whole information, just the registration status"
msgstr "не показывать всю информацию, а только состояние зарегистированности"

#: scripts/whois2:13
msgid "show info about current domain in every top domain"
msgstr "показать информацию об этом домене в каждом домене верхнего уровня"

#: scripts/whois2:14
msgid "ignore registered domains"
msgstr "игнорировать зарегистрированные домены"

#: scripts/whois2:15
msgid "ignore unregistered domains"
msgstr "игнорировать незарегистрированные доменыв"

#: scripts/whois2:16
msgid "ignore invalid domains"
msgstr "игнорировать некорректные домены"

#: scripts/whois2:17
msgid "timeout between whois queries"
msgstr "ожидание между запросами к whois"

#: scripts/whois2:18
msgid "number of simultaneous whois processes"
msgstr "количество одновременно выполняющихся процессов whois"

#: scripts/whois2:59
msgid ""
"Runtime Error: \n"
"{}\n"
msgstr ""
"Ошибка времени выполнения: \n"
"{}\n"

#: scripts/whois2:95
msgid "Domain is free to register"
msgstr "Домен свободен для регистрации"
